{
  "name": "cause-list-checker",
  "version": "0.0.1",
  "description": "The project searches for a keyword through all the cause list pdf files on a particular date.",
  "main": "src/app.js",
  "scripts": {
    "coverage-html": "cross-env NODE_ENV=localhost nyc --reporter=html mocha --recursive --exit --timeout 20000",
    "start": "node src/app.js",
    "start-local": "cross-env NODE_ENV=localhost nodemon --inspect src/app.js",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "cross-env NODE_ENV=localhost mocha --recursive --exit --timeout 20000",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karantrehan3/cause-list-checker.git"
  },
  "keywords": [
    "cause-list"
  ],
  "author": "Karan Trehan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karantrehan3/cause-list-checker/issues"
  },
  "homepage": "https://github.com/karantrehan3/cause-list-checker#readme",
  "engines": {
    "node": ">=14.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --ext .js --fix"
    ]
  },
  "nyc": {
    "all": true
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sinon": "^13.0.1"
  },
  "dependencies": {
    "selenium-webdriver": "^4.1.1"
  }
}
